{
  "name": "where2-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:extension:firefox": "web-ext run --source-dir=dist --firefox",
    "dev:extension:firefox:watch": "concurrently \"WATCH=true vite build --watch\" \"web-ext run --source-dir=dist --firefox --watch-files=dist/**/*\"",
    "preview": "vite preview",
    "build": "vite build",
    "release:firefox": "pnpm run build && web-ext build --source-dir dist --artifacts-dir dist --overwrite-dest",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "test": "vitest run",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@react-leaflet/core": "^3.0.0",
    "@tanstack/react-query": "^5.76.1",
    "@turf/turf": "^7.2.0",
    "antd": "^5.25.2",
    "axios": "^1.9.0",
    "leaflet": "^1.9.4",
    "leaflet-plugins": "^3.4.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "react-leaflet": "^5.0.0",
    "react-leaflet-google-layer": "^4.0.0",
    "zod": "^3.25.7",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@testing-library/react": "^16.3.0",
    "@types/google.maps": "^3.58.1",
    "@types/jest": "^29.5.14",
    "@types/leaflet": "^1.9.18",
    "@types/node": "^22.15.19",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@types/yandex-maps": "^2.1.36",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.27.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.1.0",
    "only-allow": "^1.2.1",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.32.1",
    "vite": "^6.3.5",
    "vite-plugin-static-copy": "^3.0.0",
    "vitest": "^3.1.4",
    "web-ext": "^8.6.0"
  },
  "packageManager": "pnpm@10.7.1",
  "engines": {
    "node": ">=16.0.0",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  }
}
